#include <iostream>
#include<iomanip>
#include<math.h>
#include<cmath>
#include<Algorithm>
using namespace std;
 
int main()
{ 
          
long long A, B;
char S;
cin >> A >> S >> B;
if (S == '+') 
    cout << A + B;
else if(S == '-')
cout << A - B;
else if (S == '*')
cout << A * B;
else if (S == '/')
cout << A / B;
 
 
}
