#include<Algorithm>
#include<stdio.h>
using namespace std;

int main(){
       
       long long x; long long y;
         cin >> x >> y;
         cout << x << " + " << y << " = " << x + y << "\n"
             << x << " * " << y << " = " << x * y << "\n"
             << x << " - " << y << " = " << x - y << "\n";
}
